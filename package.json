{
  "name": "yr-rust-konva",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "rsw build && tsc && vite build",
    "rsw": "rsw",
    "preview": "vite preview",
    "lint:eslint": "eslint --fix --ext .js,.ts,.tsx ./src",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,ts,json,tsx,css,less,scss,html,md}\"",
    "lint:stylelint": "stylelint --fix \"**/*.{less,postcss,css,scss}\"",
    "format": "npm run lint:eslint && npm run lint:stylelint && npm run lint:prettier",
    "deploy:win": "tsc && vite build && copy dist\\index.html dist\\404.html && gh-pages -d dist",
    "deploy:mac": "tsc && vite build && cp dist/index.html dist/404.html && gh-pages -d dist",
    "deploy:win:dist": "copy dist\\index.html dist\\404.html && gh-pages -d dist",
    "deploy:mac:dist": "cp dist/index.html dist/404.html && gh-pages -d dist"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write --loglevel warn"
    ],
    "src/**/*.{less,postcss,css,scss}": [
      "stylelint --fix --custom-syntax postcss-less --cache --cache-location node_modules/.cache/stylelint/"
    ]
  },
  "dependencies": {
    "@mui/material": "^5.10.10",
    "ahooks": "^3.7.0",
    "axios": "^0.27.2",
    "browser-fs-access": "^0.31.1",
    "i18next": "^21.9.0",
    "i18next-browser-languagedetector": "^6.1.5",
    "i18next-http-backend": "^1.4.1",
    "konva": "^8.3.13",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.18.3",
    "react-router-dom": "^6.3.0",
    "react-router-i18n": "^1.1.1",
    "vite-plugin-rsw": "2.0.8"
  },
  "devDependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@types/lodash": "^4.14.186",
    "@types/node": "^18.7.8",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "@vitejs/plugin-legacy": "^2.0.1",
    "@vitejs/plugin-react": "^2.0.1",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^4.0.0",
    "lint-staged": "^13.0.3",
    "path": "^0.12.7",
    "prettier": "^2.7.1",
    "sass": "^1.54.5",
    "styled-components": "^5.3.5",
    "stylelint": "^14.10.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^27.0.0",
    "stylelint-scss": "^4.3.0",
    "terser": "^5.15.1",
    "typescript": "^4.6.4",
    "vite": "^3.1.8",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-imp": "^2.2.0",
    "vite-plugin-svgr": "^2.2.1",
    "vite-plugin-webpackchunkname": "^0.1.1"
  }
}
